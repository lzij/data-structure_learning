public class recursive {
    public static void main(String[] args) {
        for(int i = 1;i<= 20;++i){
            System.out.println(getFibo(i)+"\t");
            if(i%5 ==0)
                System.out.println();
        }
    }

    private static int getFibo(int n){
        if(n == 1) return 1;
        else if(n== 2)return 2;
        else return getFibo(n-1)+getFibo(n-2);
    }
}
